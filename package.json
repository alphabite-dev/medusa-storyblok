{
  "name": "@alphabite/medusa-storyblok",
  "version": "0.2.0",
  "description": "Alphabite's Medusa Storyblok Plugin",
  "author": "Alphabite (https://alphabite.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alphabite-dev/medusa-storyblok/issues"
  },
  "homepage": "https://github.com/alphabite-dev/medusa-storyblok#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/alphabite-dev/medusa-storyblok.git"
  },
  "files": [
    ".medusa/server",
    ".medusa/types"
  ],
  "main": "./.medusa/server/src/index.js",
  "types": "./.medusa/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./.medusa/types/index.d.ts",
      "import": "./.medusa/server/src/index.js",
      "require": "./.medusa/server/src/index.js",
      "default": "./.medusa/server/src/index.js"
    },
    "./package.json": "./package.json",
    "./workflows": "./.medusa/server/src/workflows/index.js",
    "./.medusa/server/src/modules/*": "./.medusa/server/src/modules/*/index.js",
    "./modules/*": "./.medusa/server/src/modules/*/index.js",
    "./*": "./.medusa/server/src/*.js",
    "./admin": {
      "import": "./.medusa/server/src/admin/index.mjs",
      "require": "./.medusa/server/src/admin/index.js",
      "default": "./.medusa/server/src/admin/index.js"
    }
  },
  "keywords": [
    "medusa",
    "plugin",
    "medusa-plugin-other",
    "medusa-plugin",
    "medusa-v2",
    "medusa-alphabite",
    "medusa-alphabite-plugin",
    "medusa-alphabite-storyblok-plugin",
    "medusa-storyblok"
  ],
  "scripts": {
    "build": "medusa plugin:build",
    "dev": "medusa plugin:develop",
    "build:types": "tsc --project tsconfig.types.json",
    "prepublishOnly": "medusa plugin:build"
  },
  "devDependencies": {
    "@medusajs/admin-sdk": "2.11.2",
    "@medusajs/cli": "2.11.2",
    "@medusajs/framework": "2.11.2",
    "@medusajs/icons": "2.11.2",
    "@medusajs/medusa": "2.11.2",
    "@medusajs/test-utils": "2.11.2",
    "@medusajs/ui": "4.0.25",
    "@swc/core": "1.5.7",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "@medusajs/admin-sdk": "2.11.2",
    "@medusajs/cli": "2.11.2",
    "@medusajs/framework": "2.11.2",
    "@medusajs/icons": "2.11.2",
    "@medusajs/medusa": "2.11.2",
    "@medusajs/test-utils": "2.11.2",
    "@medusajs/ui": "4.0.25"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "image-size": "^2.0.2",
    "storyblok-js-client": "^7.2.1"
  }
}
